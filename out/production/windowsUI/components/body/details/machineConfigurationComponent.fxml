<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<BorderPane minHeight="258.0" minWidth="430.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.body.details.MachineConfigurationController">
   <top>
      <HBox alignment="CENTER_LEFT" minHeight="30.0" spacing="50.0" BorderPane.alignment="CENTER">
         <children>
            <StackPane maxWidth="200.0" HBox.hgrow="SOMETIMES">
               <children>
                  <Label text="Machine Configuration">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
               </children>
            </StackPane>
            <StackPane HBox.hgrow="SOMETIMES">
               <children>
                  <Label fx:id="stringConfiguration">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
               </children>
            </StackPane>
         </children>
         <padding>
            <Insets left="10.0" />
         </padding>
      </HBox>
   </top>
   <center>
      <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <content>
            <GridPane minHeight="225.0">
              <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <StackPane fx:id="reflectorPane" />
                  <StackPane fx:id="plugBoardPane" GridPane.columnIndex="2" />
                  <HBox fx:id="rotorsPane" alignment="CENTER" minHeight="225.0" minWidth="110.0" spacing="10.0" GridPane.columnIndex="1">
                     <padding>
                        <Insets left="10.0" right="10.0" />
                     </padding>
                  </HBox>
               </children>
            </GridPane>
         </content>
      </ScrollPane>
   </center>
</BorderPane>
