<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<ScrollPane fitToHeight="true" fitToWidth="true" minHeight="50.0" minWidth="125.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.ContestDetailsTableController">
   <content>
      <TableView fx:id="detailsTable" minHeight="200.0" minWidth="402.0">
        <columns>
          <TableColumn maxWidth="200.0" minWidth="75.0" prefWidth="-1.0" text="Battle Field" />
          <TableColumn maxWidth="200.0" minWidth="100.0" prefWidth="-1.0" text="Contest Manager" />
            <TableColumn maxWidth="200.0" minWidth="75.0" prefWidth="-1.0" text="Status" />
            <TableColumn maxWidth="200.0" minWidth="75.0" prefWidth="-1.0" text="Level" />
            <TableColumn maxWidth="200.0" minWidth="75.0" prefWidth="-1.0" text="Teams" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </content>
</ScrollPane>
