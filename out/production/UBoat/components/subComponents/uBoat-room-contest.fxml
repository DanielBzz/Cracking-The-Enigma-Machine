<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.subComponents.UBoatRoomContestController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="600.0" />
    <ColumnConstraints hgrow="SOMETIMES" />
  </columnConstraints>
  <rowConstraints>
      <RowConstraints valignment="BOTTOM" vgrow="SOMETIMES" />
    <RowConstraints valignment="BOTTOM" vgrow="SOMETIMES" />
    <RowConstraints valignment="BOTTOM" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <HBox alignment="CENTER_RIGHT" spacing="40.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
         <children>
            <Button fx:id="readyButton" minWidth="60.0" mnemonicParsing="false" onAction="#readyButtonListener" text="Ready!" />
            <Button fx:id="logoutButton" mnemonicParsing="false" onAction="#logoutButtonListener" text="Loguot" />
         </children>
         <GridPane.margin>
            <Insets bottom="15.0" left="10.0" />
         </GridPane.margin>
      </HBox>
    <AnchorPane>
         <children>
            <fx:include fx:id="machineConfigurationComponent" source="../../../../../windowsUI/src/components/body/details/machineConfigurationComponent.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
         </children>
      </AnchorPane>
      <AnchorPane GridPane.rowIndex="1">
         <children>
            <fx:include fx:id="encryptComponent" source="../../../../../windowsUI/src/components/body/main/encryptComponent.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
         </children>
      </AnchorPane>
      <AnchorPane fx:id="connectedTeamsPlace" GridPane.columnIndex="1" GridPane.rowSpan="2" />
      <GridPane GridPane.rowIndex="2">
        <columnConstraints>
          <ColumnConstraints hgrow="NEVER" minWidth="30.0" />
          <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <AnchorPane fx:id="candidatesTablePlace" GridPane.columnIndex="1" />
            <Label text="Candidates: ">
               <GridPane.margin>
                  <Insets left="5.0" right="5.0" />
               </GridPane.margin>
            </Label>
         </children>
      </GridPane>
   </children>
</GridPane>
