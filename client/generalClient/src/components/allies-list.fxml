<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<ScrollPane fitToHeight="true" fitToWidth="true" minHeight="50.0" minWidth="100.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.AlliesListController">
   <content>
      <TableView fx:id="alliesTable" minHeight="200.0" minWidth="177.0" prefHeight="200.0">
        <columns>
          <TableColumn maxWidth="250.0" minWidth="75.0" prefWidth="-1.0" text="Allies Name" />
          <TableColumn maxWidth="200.0" minWidth="50.0" prefWidth="-1.0" text="Amount of Agents" />
            <TableColumn maxWidth="200.0" minWidth="50.0" prefWidth="-1.0" text="Task Size" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </content>
</ScrollPane>
