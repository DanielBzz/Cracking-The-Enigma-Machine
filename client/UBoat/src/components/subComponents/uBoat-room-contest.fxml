<GridPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.subComponents.UBoatRoomContestController">
  <columnConstraints>
    <ColumnConstraints hgrow="ALWAYS" />
    <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" />
  </columnConstraints>
  <rowConstraints>
      <RowConstraints valignment="BOTTOM" vgrow="SOMETIMES" />
    <RowConstraints valignment="BOTTOM" vgrow="SOMETIMES" />
    <RowConstraints valignment="BOTTOM" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <AnchorPane GridPane.rowIndex="2">
         <children>
            <HBox alignment="CENTER_LEFT" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <Label text="Candidates:" />
                  <AnchorPane minHeight="-Infinity" minWidth="-Infinity" />
                  <fx:include fx:id="candidatesTableComponent" prefHeight="200.0" source="../../../../generalClient/src/components/candidates-table.fxml" />
               </children>
            </HBox>
         </children>
      </AnchorPane>
      <HBox alignment="BOTTOM_LEFT" spacing="40.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
         <children>
            <Button fx:id="readyButton" minWidth="60.0" mnemonicParsing="false" onAction="#readyButtonListener" text="Ready!" />
            <Button fx:id="logoutButton" mnemonicParsing="false" onAction="#logoutButtonListener" text="Loguot" />
         </children>
         <GridPane.margin>
            <Insets bottom="15.0" left="10.0" />
